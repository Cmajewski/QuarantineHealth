<%= link_to('Log in with Facebook!', '/auth/facebook') %>
<h1>Login</h1>
<% if flash[:message] %>
  <div class="warning"><%= flash[:message] %></div>
<% end %>

<%=form_for :user do |f|%>
<%=f.label :email%>
<%=f.email_field :email%><br>
<%=f.label :password%>
<%=f.password_field :password%><br>
<%=f.submit 'Login'%>
<%end%>
<br>
